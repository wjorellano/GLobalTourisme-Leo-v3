						Actif=getV3Details.get("Actif")
						Taxes=taxesID
						V3_Record_ID=v3RecordID
					];
				}
			}
			if(getMaster.Form_Name == "transport")
			{
				getV3Details = global_tourisme_zcopy.forLeo.getMasterDetails(getMaster.Record_ID.tolong(),"transport");
				//{Description_ES=, Info_TGV_et_Voucher_ES=, HIDDEN_PAYS=, Record_Status=UNLOCKED, Actif=true, Pays=[3378952000008213769], Etat_Province=[3378952000008350804], Info_prestation_PT=, HIDDEN_ETAT=, HIDDEN_CRM_ID_IMPORT=, Added_User=genevive6, Info_prestation_FR=, Liste_de_prix=[3378952000020409871], ID=3378952000020409875, Info_TGV_et_Voucher_PT=, Description_EN=, Ville=null, Added_Time=2024-07-11 12:35:18.0, Minimum_pax_tarif_groupe=0, Modified_User=emmanuelle6, HIDDEN_Nom_Fournisseur=La Buchette, Modified_Time=2024-07-30 10:30:49.0, Info_TGV_et_Voucher_FR=, Type_de_repas=[Dîner, Souper], Notes_generales=, Nom_fournisseur=[2217845000064670001], HIDDEN_VILLE=, Description_PT=, Info_prestation_EN=, Info_TGV_et_Voucher_EN=, Info_prestation_ES=, Description_FR=}
				if(getV3Details.size() > 0)
				{
					v3RecordID = getV3Details.get("ID");
					findExistingRecordTransport = Transport[V3_Record_ID == v3RecordID];
					Nom_fournisseur = getV3Details.get("Fournisseur_CRM");
					Nom_fournisseurID = null;
					if(Nom_fournisseur != null)
					{
						getVendors = Vendors[CRM_ID == Nom_fournisseur];
						Nom_fournisseurID = getVendors.ID;
					}
					pays = getV3Details.get("Pays");
					payID = null;
					if(pays != null)
					{
						getPaysLeo = Pays[V3_Record_ID == pays.tostring()];
						payID = getPaysLeo.ID;
					}
					EtatProvince = getV3Details.get("Etat_Province");
					Etat_ProvinceID = null;
					if(EtatProvince != null)
					{
						getEtat = Etat_Province[V3_Record_ID == EtatProvince.tostring()];
						Etat_ProvinceID = getEtat.ID;
					}
					villeV3 = getV3Details.get("Ville");
					villeID = null;
					if(villeV3 != null)
					{
						getVille = Ville[V3_Record_ID == villeV3.tostring()];
						villeID = getVille.ID;
					}
					if(findExistingRecordTransport.count() > 0)
					{
						findExistingRecordTransport.Fournisseur_CRM1=Nom_fournisseurID;
						findExistingRecordTransport.Nom=getV3Details.get("Nom");
						findExistingRecordTransport.Pays=payID;
						findExistingRecordTransport.Actif=getV3Details.get("Actif");
						findExistingRecordTransport.Type1=getV3Details.get("Type1");
						findExistingRecordTransport.Info_TGV_et_Voucher_FR=getV3Details.get("Info_TGV_et_Voucher_FR");
						findExistingRecordTransport.Info_TGV_et_Voucher_ES=getV3Details.get("Info_TGV_et_Voucher_ES");
						findExistingRecordTransport.Etat_Province=Etat_ProvinceID;
						findExistingRecordTransport.Ville=villeID;
						findExistingRecordTransport.Info_TGV_et_Voucher_EN=getV3Details.get("Info_TGV_et_Voucher_EN");
						findExistingRecordTransport.Info_TGV_et_Voucher_PT=getV3Details.get("Info_TGV_et_Voucher_PT");
						findExistingRecordTransport.Notes_g_n_rales=getV3Details.get("Notes_generales");
						findExistingRecordTransport.HIDDEN_CRM_ID_IMPORT=getV3Details.get("HIDDEN_CRM_ID_IMPORT");
						findExistingRecordTransport.HIDDEN_PAYS=getV3Details.get("HIDDEN_PAYS");
						findExistingRecordTransport.HIDDEN_ETAT=getV3Details.get("HIDDEN_ETAT");
						findExistingRecordTransport.HIDDEN_VILLE=getV3Details.get("HIDDEN_VILLE");
					}
					else
					{
						repID = insert into Transport
						[
							Added_User=zoho.loginuser
							Fournisseur_CRM1=Nom_fournisseurID
							Nom=getV3Details.get("Nom")
							Pays=payID
							Actif=getV3Details.get("Actif")
							Type1=getV3Details.get("Type1")
							Info_TGV_et_Voucher_FR=getV3Details.get("Info_TGV_et_Voucher_FR")
							Info_TGV_et_Voucher_ES=getV3Details.get("Info_TGV_et_Voucher_ES")
							Etat_Province=Etat_ProvinceID
							Ville=villeID
							Info_TGV_et_Voucher_EN=getV3Details.get("Info_TGV_et_Voucher_EN")
							Info_TGV_et_Voucher_PT=getV3Details.get("Info_TGV_et_Voucher_PT")
							Notes_g_n_rales=getV3Details.get("Notes_generales")
							HIDDEN_CRM_ID_IMPORT=getV3Details.get("HIDDEN_CRM_ID_IMPORT")
							HIDDEN_PAYS=getV3Details.get("HIDDEN_PAYS")
							HIDDEN_ETAT=getV3Details.get("HIDDEN_ETAT")
							HIDDEN_VILLE=getV3Details.get("HIDDEN_VILLE")
							V3_Record_ID=v3RecordID
						];
					}
				}
			}
			getMaster.Status="processed";
		}
		catch (e)
		{
			getMaster.Error_Message=e;
			info e;
		}
	}
}
